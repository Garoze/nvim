{
    "html.autoClosingTags": true,
    "html.suggest.html5": true,
    "html.filetypes": [
        "html",
        "handlebars",
        "htmldjango",
        "tsx",
        "jsx"
    ],
    "pairs.enableCharacters": [
        "(",
        "[",
        "{",
        "'",
        "\"",
        "`"
    ],
    "suggest.timeout": 3000,
    "languageserver": {
        "clangd": {
            "command": "clangd",
            "rootPatterns": [
                "compile_flags.txt",
                "compile_commands.json"
            ],
            "filetypes": [
                "c",
                "cc",
                "cpp",
                "c++",
                "objc",
                "objcpp"
            ]
        },
        "lua": {
            "command": "lua-lsp",
            "filetypes": [
                "lua"
            ]
        }
    },
    "coc.preferences.formatOnSaveFiletypes": [
        "javascript",
        "json",
        "python"
    ],
    "snippets.ultisnips.directories": [
        "UltiSnips",
        "~/.config/nvim/complete/"
    ]
}
